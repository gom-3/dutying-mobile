{
  "expo": {
    "name": "dutying-mobile",
    "slug": "dutying-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-apple-authentication",
      "@react-native-firebase/app",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar.",
          "remindersPermission": "The app needs to access your reminders."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share them with your friends."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@react-native-seoul/kakao-login",
        {
          "kakaoAppKey": "1bcf231af91f55cd3ca892cce7dce246",
          "overrideKakaoSDKVersion": "2.11.1",
          "kotlinVersion": "1.9.0"
        }
      ],
      [
        "airbridge-expo-sdk",
        {
          "appName": "dutying",
          "appToken": "cfde524ff73c47a8a5c863327579af6f"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "infoPlist": {
        "NSCalendarsUsageDescription": "The app needs to access your calendar.",
        "NSRemindersUsageDescription": "The app needs to access your reminders."
      },
      "bundleIdentifier": "com.gom3.dutying"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.gom3.dutying",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4f3009ff-f032-4d32-b711-054b0d76e953"
      }
    }
  }
}
